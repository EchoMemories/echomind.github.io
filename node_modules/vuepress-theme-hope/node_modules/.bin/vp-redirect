#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Desktop/chat/echomind-docs/node_modules/.pnpm/@vuepress+plugin-redirect@2_27e9a25f266fbc03cc1ce65b66b4d6e8/node_modules/@vuepress/plugin-redirect/lib/cli/node_modules:/mnt/d/Desktop/chat/echomind-docs/node_modules/.pnpm/@vuepress+plugin-redirect@2_27e9a25f266fbc03cc1ce65b66b4d6e8/node_modules/@vuepress/plugin-redirect/lib/node_modules:/mnt/d/Desktop/chat/echomind-docs/node_modules/.pnpm/@vuepress+plugin-redirect@2_27e9a25f266fbc03cc1ce65b66b4d6e8/node_modules/@vuepress/plugin-redirect/node_modules:/mnt/d/Desktop/chat/echomind-docs/node_modules/.pnpm/@vuepress+plugin-redirect@2_27e9a25f266fbc03cc1ce65b66b4d6e8/node_modules/@vuepress/node_modules:/mnt/d/Desktop/chat/echomind-docs/node_modules/.pnpm/@vuepress+plugin-redirect@2_27e9a25f266fbc03cc1ce65b66b4d6e8/node_modules:/mnt/d/Desktop/chat/echomind-docs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Desktop/chat/echomind-docs/node_modules/.pnpm/@vuepress+plugin-redirect@2_27e9a25f266fbc03cc1ce65b66b4d6e8/node_modules/@vuepress/plugin-redirect/lib/cli/node_modules:/mnt/d/Desktop/chat/echomind-docs/node_modules/.pnpm/@vuepress+plugin-redirect@2_27e9a25f266fbc03cc1ce65b66b4d6e8/node_modules/@vuepress/plugin-redirect/lib/node_modules:/mnt/d/Desktop/chat/echomind-docs/node_modules/.pnpm/@vuepress+plugin-redirect@2_27e9a25f266fbc03cc1ce65b66b4d6e8/node_modules/@vuepress/plugin-redirect/node_modules:/mnt/d/Desktop/chat/echomind-docs/node_modules/.pnpm/@vuepress+plugin-redirect@2_27e9a25f266fbc03cc1ce65b66b4d6e8/node_modules/@vuepress/node_modules:/mnt/d/Desktop/chat/echomind-docs/node_modules/.pnpm/@vuepress+plugin-redirect@2_27e9a25f266fbc03cc1ce65b66b4d6e8/node_modules:/mnt/d/Desktop/chat/echomind-docs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@vuepress+plugin-redirect@2_27e9a25f266fbc03cc1ce65b66b4d6e8/node_modules/@vuepress/plugin-redirect/lib/cli/index.js" "$@"
else
  exec node  "$basedir/../../../../../@vuepress+plugin-redirect@2_27e9a25f266fbc03cc1ce65b66b4d6e8/node_modules/@vuepress/plugin-redirect/lib/cli/index.js" "$@"
fi
